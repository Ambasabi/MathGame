<Window x:Class="MathGame.HighScoresWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModels="clr-namespace:MathGame.ViewModels"
        Title="HighScoresWindow" Height="263.782" Width="376.159">
    <Window.Background>
        <ImageBrush ImageSource="Images/HighScores.jpg"/>
    </Window.Background>
    <Grid HorizontalAlignment="Left" Width="589">

        <DataGrid ItemsSource="{Binding Path=HighScores}" AutoGenerateColumns="True" Height="187" HorizontalAlignment="Left" Margin="10,10,0,0" Name="dataGrid1" VerticalAlignment="Top" Width="352" OpacityMask="#7F000000" />
        <Button x:Name="cmdCloseHighScores" Content="Close" HorizontalAlignment="Left" Margin="137,202,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
